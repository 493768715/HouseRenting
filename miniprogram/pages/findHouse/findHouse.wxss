.top{
    position: fixed;
    width: 100%;
}
.search{
    width: 100%;
    height: 100rpx;
    display: flex;
    background-color: #fff;
    justify-content: space-between;
    align-items: center;
}
.search .loc{
    width: 250rpx;
    padding-left: 20rpx;
}
.search image{
    width: 25rpx;
    padding-right: 10rpx;
}
.loc text{
    font-size: 28rpx;
    color: #666666;
}
.search .searchInfo{
    display: flex;
    position: relative;
    width: 450rpx;
    height: 60rpx;
}
.search .sinfo{
    background-color: #F3F4F8;
    position: absolute;
    width: 400rpx;
    height: 60rpx;
    font-size: 30rpx;
    border-radius: 10rpx;
    padding: 0 20rpx; 
}
.search .spic{
    width: 40rpx;
    position: absolute;
    top: 10rpx;
    right: 15rpx;
    z-index: 999999;
}
.Htype{
    background-color: #fff;
    display: flex;
    height: 100rpx;
    width: 100%;
    margin-bottom: 30rpx;
}
.Htype .type{
    text-align: center;
    width: 25%;
    color: #7A7A7A;
    font-size: 30rpx;
    border-right: 1rpx dashed #DCDCDC;
}

.picker image{
    width: 25rpx;
    padding-left: 10rpx ;
}
.type .picker{
    padding: 30rpx 0;
    border-right:1prx solid red ;
}
.type .info{
    font-size: 28rpx;
    padding: 30rpx 0;
}

.result{
    height: 100%;
    width: 100%;
    padding-top: 200rpx;
}
.result navigator{
    display: flex;
    align-items: center;
    height: 260rpx;
    background-color: #fff;
    margin:10rpx 20rpx 10rpx 20rpx;
    color: #919191;
}
.result image{
    width: 280rpx;
    padding: 0rpx 10rpx;
}
.Ritem{
    padding-left: 10rpx;
}
.Ritem .tit{
    font-size: 30rpx;
    padding-right: 10rpx;
    color: #5B5B5B;
    font-weight: bold;
    padding-bottom: 10rpx;
}
.Ritem .type{
    font-size: 26rpx;
}
.Ritem .money{
    font-size: 30rpx;
    padding: 10rpx 0;
    color: #F08D17;   
    
}
.Ritem .time{
    font-size: 26rpx;

}