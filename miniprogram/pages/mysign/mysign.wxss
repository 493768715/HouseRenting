.img{
    width: 100%;
    height: 100%;  
    text-align: center;
}
.box{
    width:100%;
    height: 100%;
    background-color: #eee;
    border-top: 1rpx solid #eee;
    padding-bottom: 20rpx;
}
.item{
    background-color: #fff;
    height: 160rpx;
    width: 660rpx;
    margin: 20rpx;
    padding: 20rpx;
    line-height: 50rpx;
}
.item .tit{
    font-size: 36rpx;
    font-weight: bold;
}
.item .person{
    display: flex;
    justify-content: space-between;
    padding: 10rpx 20rpx 10rpx 0;
    font-size: 30rpx;
    color: #808080;
    
}
.item .crtime{
    display: flex;
    justify-content: space-between;
    font-size: 30rpx;
    color: #808080;
}
.crtime .break{
    color: #35BAF1;
    padding-right: 20rpx;
}