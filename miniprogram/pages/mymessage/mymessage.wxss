.img{
    width: 100%;
    height: 100%;  
    text-align: center;
}
.box{
    width:100%;
    height: 100%;
    background-color: #eee;
    border-top: 1rpx solid #eee;
    padding-bottom: 20rpx;
}
.item{
    background-color: #fff;
    margin: 20rpx;
    padding: 20rpx;
}
.item .tit{
    font-size: 35rpx;
    font-weight: bold;
}
.item .title{
    padding: 20rpx 0 10rpx 0;
    font-size: 30rpx;
}
.item .time{
    font-size: 30rpx;
    color: #808080;
}